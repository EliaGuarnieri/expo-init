{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "checkJs": true,
    "target": "ES2015",
    "module": "commonjs",
    "jsx": "react-native",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": [
      "es2015",
      "esnext.asynciterable"
    ],
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
    },
  },
  "include": [
    "src/**/*"
  ],
}