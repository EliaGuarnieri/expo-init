{
  "name": "expo-init",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --android --localhost",
    "dev:clean": "expo start --android --localhost --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "debugger": "pwsh.exe -c './scripts/rndebugger/react-native-debugger.exe --no-sandbox' >> /dev/null 2>&1 &"
  },
  "dependencies": {
    "expo": "^47.0.0",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-native": "0.70.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "~9.2.1",
    "eslint": "^8.29.0",
    "eslint-config-universe": "^11.1.1",
    "prettier": "^2.8.1"
  },
  "overrides": {
    "react-native": {
      "react-devtools-core": "4.14.0"
    }
  },
  "eslintConfig": {
    "extends": "universe"
  },
  "private": true
}
